{
  "presets": [["react-app", { "flow": false, "typescript": true }]],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "src": "./src"
        },
        "cwd": "babelrc"
      }
    ],
    "@babel/plugin-proposal-class-properties"
  ]
}
